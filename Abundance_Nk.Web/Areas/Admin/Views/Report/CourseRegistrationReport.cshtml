<script src="~/Scripts/jquery.min.js"></script>
<script src="~/Scripts/responsive-tabs.js"></script>

<script type="text/javascript">
    function resizeIframe(obj) {
        obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
    }
</script>



<div class="col-md-11 ml-auto mr-auto">

    <div class="card card-shadow">
        <div class="card-header">
            <h3>
                Course Registration Report
            </h3>
        </div>
        <div class="card-body">
           

            <div class="alert alert-dismissible alert-success">
                <p>
                    <b>Course Registration Report</b> Shows Registered Courses in a Department for a given Session.
                </p>
            </div>

            <iframe src="~/Reports/Presenter/CourseRegistrationReport.aspx" style="width:100%; height:auto; " frameborder="0" scrolling="no" id="iframe" onload='javascript:resizeIframe(this);'></iframe>


            <br />
        </div>
    </div>
</div>
