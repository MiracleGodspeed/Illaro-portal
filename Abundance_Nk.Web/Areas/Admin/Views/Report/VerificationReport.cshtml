@{
    ViewBag.Title = "ApplicantResult";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<script src="~/Scripts/jquery.min.js"></script>
<script src="~/Scripts/responsive-tabs.js"></script>

<script type="text/javascript">
    function resizeIframe(obj) {
        obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
    }
</script>


<div class="col-md-10 ml-auto mr-auto">
    <div class="card card-shadow">
        <div class="card-header"><h3>
    Verification Report
</h3></div>

        <div class="card-body">
          

            <div class="alert alert-dismissible alert-success">
                <p>
                    <b> Verification Report</b> displays applicants verification status and details.
                </p>
            </div>

            <iframe src="~/Reports/Presenter/VerificationReport.aspx" style="width:100%; " frameborder="0" scrolling="no" id="iframe" onload='javascript:resizeIframe(this);'></iframe>

        </div>
    </div>
</div>
