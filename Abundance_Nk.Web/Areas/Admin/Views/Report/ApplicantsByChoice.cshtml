@{
    ViewBag.Title = "ApplicantsByChoice";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<script src="~/Scripts/jquery.min.js"></script>
<script src="~/Scripts/responsive-tabs.js"></script>

<script type="text/javascript">
    function resizeIframe(obj) {
        obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
    }
</script>



<div class="col-md-12 ml-auto mr-auto">

    <div class="card card-shadow">
        <div class="card-header">
            <h3>
                Applicants By Choice
            </h3>
        </div>

        <div class="card-body">
            

            <div class="alert alert-dismissible alert-success">
                <p>
                    <b> Applicants By Choic</b> displays applicants result details according to the institution choice.
                </p>
            </div>

            <iframe src="~/Reports/Presenter/Result/ApplicantsByChoice.aspx" style="width:100%; " frameborder="0" scrolling="no" id="iframe" onload='javascript:resizeIframe(this);'></iframe>

            
        </div>

        </div>

    </div>
        

