@model Abundance_Nk.Model.Model.Invoice

@{
    Layout = null;
}



@if (TempData["Message"] != null)
{
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <br />
                @Html.Partial("_Message", (Abundance_Nk.Model.Model.Message)TempData["Message"])
            </div>
        </div>
    </div>
}

@if(Model != null && Model.paymentScholarship == null)
{
    @Html.Partial("_Invoice", Model)
}
else
{
    @Html.Partial("_InvoiceScholarship", Model)
}


