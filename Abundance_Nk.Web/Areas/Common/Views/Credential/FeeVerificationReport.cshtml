@{
    ViewBag.Title = "Verification Report";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}
<script src="~/Scripts/jquery-2.1.3.min.js"></script>
<script src="~/Scripts/responsive-tabs.js"></script>
<script type="text/javascript">
    function resizeIframe(obj) {
        obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
    }
</script>


<div class="col-md-12 ml-auto mr-auto">
    <div class="card card-shadow">
        <div class="card-header">
            <h3>
                Verified Payment Report
            </h3>
        </div>


        <div class="card-body">
            <div class="alert alert-dismissible alert-success">
                <p>
                    <b>Verified Payment Report</b> Shows the verified Payment Report.
                </p>
            </div>
            <iframe src="@Url.Content("~/Reports/Presenter/FeeVerificationReport.aspx?sessionId=" + ViewBag.sessionId + "&levelId=" + ViewBag.levelId + "&progId=" + ViewBag.progId + "&deptId=" + ViewBag.deptId + "&semesterId=" + ViewBag.semesterId + "&courseId=" + ViewBag.courseId)" style="width: 100%;" frameborder="0" scrolling="no" id="iframe" onload=' javascript:resizeIframe(this); '></iframe>

        </div>
    </div>
</div>
