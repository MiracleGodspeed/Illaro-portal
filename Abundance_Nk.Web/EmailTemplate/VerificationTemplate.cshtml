@model Abundance_Nk.Web.Areas.Admin.ViewModels.TranscriptProcessorViewModel
<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">

<!-- Mirrored from wrappixel.com/ampleadmin/ampleadmin-html/email-templates/basic.html by HTTrack Website Copier/3.x [XR&CO'2014], Thu, 31 May 2018 16:33:08 GMT -->
<head>
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>Verification Notification</title>
</head>
<body style="margin: 0px; background: #f8f8f8;">
    <div width="100%" style="background: #f8f8f8; padding: 0px 0px; font-family: arial; line-height: 28px; height: 100%; width: 100%; color: #514d6a;">
        <div style="max-width: 700px; padding: 50px 0; margin: 0px auto; font-size: 14px">
            <table border="0" cellpadding="0" cellspacing="0" style="width: 100%; margin-bottom: 20px">
                <tbody>
                    <tr>
                        <td style="vertical-align: top; padding-bottom: 30px;" align="center">
                            <a href="javascript:void(0)" target="_blank">
                                <img src="http://applications.federalpolyilaro.edu.ng/Content/Images/school_logo.jpg" alt="schoolLogo" =kit" style="border: none; height: 50px; width: 50px" />
                            </a>
                        </td>
                    </tr>
                </tbody>
            </table>
            <div style="padding: 40px; background: #fff;">
                <table border="0" cellpadding="0" cellspacing="0" style="width: 100%;">
                    <tbody>
                        <tr>

                            <td>
                                <b>Dear @Model.Student.FullName,</b>
                                <p>This is to notify you that your Wes Verification has be Processed. Thank you.</p>
                                <b>- Thanks (R&amp;DC)</b>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div style="text-align: center; font-size: 12px; color: #b2b2b5; margin-top: 20px">
                <p>
                    Powered by LLoydant Business Services <br>
                </p>
            </div>
        </div>
    </div>
